+void ChromiumBrowser::setBrowser(CefBrowser* browser)
+{
+	m_pBrowser = browser;
+
+	if (m_iLastTask == 1)
+	{
+		loadString(m_szBuffer.c_str());
+	}
+	else if (m_iLastTask == 2)
+	{
+		loadUrl(m_szBuffer.c_str());
+	}
+
+	m_szBuffer = "";
+
+#ifdef WIN32
+	onResize();
+#endif
+}
