+CefRefPtr<CefV8Value> JavaScriptObject::getCefV8()
+{
+	return m_pObject;
+}
+
+CefRefPtr<CefBase> JavaScriptObject::getCefBase()
+{
+	CefRefPtr<CefBase> base(new V8ValueBaseWrapper(m_pObject));
+	return base;
+}
